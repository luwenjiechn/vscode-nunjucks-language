{
  "name": "nunjucks-language",
  "displayName": "Nunjucks Language",
  "description": "VSCode Nunjucks Language",
  "icon": "images/icon.png",
  "version": "0.0.5",
  "publisher": "luwenjiechn",
  "repository": {
    "type": "git",
    "url": "https://github.com/luwenjiechn/vscode-nunjucks-language"
  },
  "bugs": {
    "url": "https://github.com/luwenjiechn/vscode-nunjucks-language/issues"
  },
  "homepage": "https://github.com/luwenjiechn/vscode-nunjucks-language#readme",
  "engines": {
    "vscode": "^1.23.0"
  },
  "keywords": [
    "Nunjucks",
    "Snippets",
    "Languages",
    "Format",
    "Syntax"
  ],
  "categories": [
    "Programming Languages",
    "Snippets",
    "Formatters",
    "Other"
  ],
  "activationEvents": [
    "onLanguage:nunjucks"
  ],
  "main": "./out/extension",
  "contributes": {
    "languages": [
      {
        "id": "nunjucks",
        "aliases": [
          "Nunjucks",
          "nunjucks"
        ],
        "extensions": [
          ".nunjucks",
          ".njk",
          ".nj",
          ".tpl",
          ".html",
          ".htm"
        ],
        "configuration": "./src/languages/nunjucks.configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "nunjucks",
        "scopeName": "text.html.nunjucks",
        "path": "./src/syntaxes/nunjucks.tmLanguage"
      }
    ],
    "snippets": [
      {
        "language": "nunjucks",
        "path": "./src/snippets/snippets.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.5.2",
    "tslint": "^5.11.0",
    "typescript": "^2.9.2",
    "vscode": "^1.1.18"
  },
  "dependencies": {
    "prettydiff2": "^2.2.7"
  }
}
